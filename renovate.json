    },

    {
      "matchPackagePrefixes": ["org.xwiki.commons:", "org.xwiki.rendering:", "org.xwiki.platform:", "org.xwiki.enterprise:",
        "com.xwiki.parent:"],
      "enabled": false
    }
  ]
}
